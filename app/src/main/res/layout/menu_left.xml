<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_menu"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="start"
    android:background="@color/white"
    android:choiceMode="none"
    android:clickable="true"
    android:fillViewport="true"
    android:focusable="true">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/main_menu"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/exit"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="visible">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/header"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="UseCompoundDrawables">

            <TextView
                android:id="@+id/titleParams"
                android:layout_width="0dp"
                android:layout_height="wrap_content"


                android:layout_weight="1"
                android:fontFamily="@font/roboto_medium"
                android:paddingStart="@dimen/margin_left_menu_8"
                android:paddingTop="@dimen/margin_left_menu_image_or_title_16"
                android:paddingBottom="@dimen/margin_left_menu_image_or_title_16"
                android:text="@string/title_left_menu"
                android:textColor="@color/fonts_lt"
                android:textSize="21sp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/changeTheme"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginTop="@dimen/margin_left_menu_image_or_title_16"
                android:layout_marginEnd="@dimen/margin_left_menu_8"
                android:layout_marginBottom="@dimen/margin_left_menu_image_or_title_16"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/ic_moon" />

            <FrameLayout
                android:id="@+id/changeThemeBottom"
                android:layout_width="40dp"
                android:layout_height="0dp"
                android:layout_marginBottom="@dimen/margin_left_menu_8"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <View
                android:id="@+id/divider_left_menu_title"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/divider_lt"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/left_menu_values_module"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/header">

            <ImageView
                android:id="@+id/icon_receipts_count"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:src="@drawable/ic_left_menu_receipt_count" />

            <TextView
                android:id="@+id/title_receipts_count"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"

                android:fontFamily="@font/roboto_regular"
                android:text="@string/title_receipts_count"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="@+id/icon_receipts_count"
                app:layout_constraintLeft_toRightOf="@+id/icon_receipts_count"
                app:layout_constraintTop_toTopOf="@+id/icon_receipts_count" />

            <TextView
                android:id="@+id/value_receipts_count"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:fontFamily="@font/roboto_regular"
                android:text="@string/value_left_menu"
                android:textSize="16sp"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@id/title_receipts_count"
                tools:text="0 шт" />

            <ImageView
                android:id="@+id/icon_send_sms"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                app:layout_constraintLeft_toLeftOf="@+id/icon_receipts_count"
                app:layout_constraintTop_toBottomOf="@+id/icon_receipts_count"
                tools:src="@drawable/ic_left_menu_send_sms" />

            <TextView
                android:id="@+id/title_send_sms"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/roboto_regular"
                android:text="@string/title_send_sms"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="@+id/icon_send_sms"
                app:layout_constraintLeft_toLeftOf="@id/title_receipts_count"
                app:layout_constraintTop_toTopOf="@+id/icon_send_sms" />

            <TextView
                android:id="@+id/value_send_sms"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/roboto_regular"
                android:text="@string/value_left_menu"
                android:textSize="16sp"
                app:layout_constraintRight_toRightOf="@+id/value_receipts_count"
                app:layout_constraintTop_toTopOf="@id/title_send_sms"
                tools:text="0 шт" />

            <ImageView
                android:id="@+id/icon_send_email"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                app:layout_constraintLeft_toLeftOf="@+id/icon_send_sms"
                app:layout_constraintTop_toBottomOf="@+id/icon_send_sms"
                tools:src="@drawable/ic_left_menu_send_email" />

            <TextView
                android:id="@+id/title_send_email"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/roboto_regular"
                android:text="@string/title_send_email"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="@+id/icon_send_email"
                app:layout_constraintLeft_toLeftOf="@id/title_send_sms"
                app:layout_constraintTop_toTopOf="@+id/icon_send_email" />

            <TextView
                android:id="@+id/value_send_email"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/roboto_regular"
                android:text="@string/value_left_menu"
                android:textSize="16sp"
                app:layout_constraintRight_toRightOf="@+id/value_send_sms"
                app:layout_constraintTop_toTopOf="@id/title_send_email"
                tools:text="0 шт" />

            <View
                android:id="@+id/divider_left_menu_values_module"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:layout_marginTop="8dp"
                android:background="@color/divider_lt"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/icon_send_email"
                app:layout_constraintTop_toBottomOf="@+id/icon_send_email" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/left_menu_sim_info"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="@id/main_menu"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/left_menu_values_module">

            <ImageView
                android:id="@+id/icon_sim"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:src="@drawable/ic_left_menu_sim" />

            <TextView
                android:id="@+id/title_sim"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/margin_left_menu_8"
                android:fontFamily="@font/roboto_regular"
                android:text="@string/title_sim"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="@+id/icon_sim"
                app:layout_constraintLeft_toRightOf="@+id/icon_sim"
                app:layout_constraintTop_toTopOf="@+id/icon_sim" />

            <ImageView
                android:id="@+id/icon_about"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="@+id/icon_sim"
                tools:src="@drawable/ic_left_menu_about" />

            <TextView
                android:id="@+id/title_about"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/roboto_regular"
                android:text="@string/title_about"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="@+id/icon_about"
                app:layout_constraintLeft_toLeftOf="@+id/title_sim"
                app:layout_constraintTop_toTopOf="@+id/icon_about" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/about_menu"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:visibility="gone"
        app:layout_constraintBottom_toTopOf="@+id/exit"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="gone">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/header_about"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="UseCompoundDrawables">

            <TextView
                android:id="@+id/about_menu_title_about"
                android:layout_width="0dp"
                android:layout_height="wrap_content"


                android:layout_weight="1"
                android:fontFamily="@font/roboto_medium"
                android:paddingStart="@dimen/margin_left_menu_8"
                android:paddingTop="@dimen/margin_left_menu_image_or_title_16"
                android:paddingBottom="@dimen/margin_left_menu_image_or_title_16"
                android:text="О приложении"
                android:textColor="@color/fonts_lt"
                android:textSize="21sp"
                app:layout_constraintLeft_toRightOf="@+id/about_menu_button_back"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/about_menu_image_back"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="@dimen/margin_left_menu_image_or_title_16"
                android:layout_marginEnd="@dimen/margin_left_menu_8"
                android:layout_marginBottom="@dimen/margin_left_menu_image_or_title_16"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:srcCompat="@drawable/ic_left_menu_arrow_back" />

            <FrameLayout
                android:id="@+id/about_menu_button_back"
                android:layout_width="40dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/frame_logo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="7dp"
            app:layout_constraintTop_toBottomOf="@+id/header_about">

            <ImageView
                android:id="@+id/about_menu_big_logo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_sv_big_logo"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/about_menu_version"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:text="Версия 1.0.0 (555)"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/about_menu_big_logo" />

            <View
                android:id="@+id/about_menu_divider_title"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="15dp"
                android:background="@color/divider_lt"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/about_menu_version" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/about_menu_feedback"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="15dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/frame_logo">

            <ImageView
                android:id="@+id/about_menu_icon_feedback"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:src="@drawable/ic_icon_feedback" />

            <TextView
                android:id="@+id/about_menu_title_feedback"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:fontFamily="@font/roboto_regular"
                android:text="@string/about_menu_title_feedback"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toRightOf="@+id/about_menu_icon_feedback"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/about_menu_rate_the_app"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            app:layout_constraintLeft_toLeftOf="@+id/about_menu_feedback"
            app:layout_constraintRight_toRightOf="@+id/about_menu_feedback"
            app:layout_constraintTop_toBottomOf="@+id/about_menu_feedback">

            <ImageView
                android:id="@+id/about_menu_icon_rate_the_app"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:src="@drawable/ic_about_menu_icon_rate_the_app" />

            <TextView
                android:id="@+id/about_menu_title_rate_the_app"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:fontFamily="@font/roboto_regular"
                android:text="@string/about_menu_title_rate_the_app"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toRightOf="@+id/about_menu_icon_rate_the_app"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/about_menu_privacy_policy"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            app:layout_constraintLeft_toLeftOf="@+id/about_menu_rate_the_app"
            app:layout_constraintRight_toRightOf="@+id/about_menu_rate_the_app"
            app:layout_constraintTop_toBottomOf="@+id/about_menu_rate_the_app">

            <ImageView
                android:id="@+id/about_menu_icon_privacy_policy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:src="@drawable/ic_icon_privacy_policy" />

            <TextView
                android:id="@+id/about_menu_title_privacy_policy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:fontFamily="@font/roboto_regular"
                android:text="@string/about_menu_title_privacy_policy"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toRightOf="@+id/about_menu_icon_privacy_policy"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/about_menu_user_agreement"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            app:layout_constraintLeft_toLeftOf="@+id/about_menu_privacy_policy"
            app:layout_constraintRight_toRightOf="@+id/about_menu_privacy_policy"
            app:layout_constraintTop_toBottomOf="@+id/about_menu_privacy_policy">

            <ImageView
                android:id="@+id/about_menu_icon_user_agreement"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:src="@drawable/ic_icon_user_agreement" />

            <TextView
                android:id="@+id/about_menu_title_user_agreement"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:fontFamily="@font/roboto_regular"
                android:text="@string/about_menu_title_user_agreement"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toRightOf="@+id/about_menu_icon_user_agreement"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/about_menu_licenses"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            app:layout_constraintLeft_toLeftOf="@+id/about_menu_user_agreement"
            app:layout_constraintRight_toRightOf="@+id/about_menu_user_agreement"
            app:layout_constraintTop_toBottomOf="@+id/about_menu_user_agreement">

            <ImageView
                android:id="@+id/about_menu_icon_licenses"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:src="@drawable/ic_icon_licenses" />

            <TextView
                android:id="@+id/about_menu_title_licenses"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:fontFamily="@font/roboto_regular"
                android:text="@string/about_menu_title_licenses"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toRightOf="@+id/about_menu_icon_licenses"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/about_menu_data_protection"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            app:layout_constraintLeft_toLeftOf="@+id/about_menu_licenses"
            app:layout_constraintRight_toRightOf="@+id/about_menu_licenses"
            app:layout_constraintTop_toBottomOf="@+id/about_menu_licenses">

            <ImageView
                android:id="@+id/about_menu_icon_data_protection"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:src="@drawable/ic_icon_data_protection" />

            <TextView
                android:id="@+id/about_menu_title_data_protection"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:fontFamily="@font/roboto_regular"
                android:text="@string/about_menu_title_data_protection"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toRightOf="@+id/about_menu_icon_data_protection"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <!-- <View
             android:id="@+id/about_menu_divider_bottom"
             android:layout_width="match_parent"
             android:layout_height="1dp"
             android:layout_marginTop="15dp"
             android:background="@color/divider_lt"
             app:layout_constraintEnd_toEndOf="parent"
             app:layout_constraintStart_toStartOf="parent"
             app:layout_constraintTop_toBottomOf="@+id/about_menu_data_protection" />-->


    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/exit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/iconExit"
            android:layout_width="wrap_content"
            android:layout_height="18dp"
            android:layout_marginStart="@dimen/margin_left_menu_image_or_title_16"
            android:layout_marginTop="@dimen/margin_left_menu_8"
            android:layout_marginBottom="@dimen/margin_left_menu_8"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_exit" />

        <TextView
            android:id="@+id/titleExit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/margin_left_menu_8"
            android:fontFamily="@font/roboto_medium"
            android:text="@string/exit_up"
            android:textColor="@color/fonts_lt"
            android:textSize="@dimen/text_size_left_menu"
            app:layout_constraintBottom_toBottomOf="@+id/iconExit"
            app:layout_constraintStart_toEndOf="@+id/iconExit"
            app:layout_constraintTop_toTopOf="@+id/iconExit" />

        <View
            android:id="@+id/dividerExit"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/divider_lt"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/version"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/margin_left_menu_image_or_title_16"
            android:fontFamily="@font/roboto_medium"
            android:textColor="@color/active_fonts_lt"
            android:textSize="@dimen/text_size_version"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="v 1.0.0 (21)" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>